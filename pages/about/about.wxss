/* pages/about/about.wxss */
.body {
  width: 100%;
  height: 100%;
  background: #F3F3F3;
}


.header {
    z-index: 99;
    width: 100%;
    height: 2rem;
    line-height: 2rem;
    background: #fff;
    position: fixed;
    top: 0;
}
.text-center {
    text-align: center;
}
.header-title {
    font-size: 1rem;
    color: #333;
}
.inline {
    display: inline-block !important;
}
.logo {
    width: 120rpx;
    height: 120rpx;
    margin: 120rpx 300rpx 30rpx ;
    display: block;
  }
.container {
  padding: 20rpx;
  font-size: .875rem;
  color: #58666e;
}  
.about-m-h {
    min-height: calc(100vh );
}
.wrapper {
  margin-top: 0;
}  
.title, .slogan {
  text-align: center;
  margin-bottom: .975rem;
  font-weight: 700;
}
.desc {
  margin-bottom: .975rem;
}